      "default": "./fesm2020/extended.mjs"
    },
    "./flex": {
      "types": "./flex.d.ts",
      "esm2020": "./esm2020/flex/flex.mjs",
      "es2020": "./fesm2020/flex.mjs",
      "es2015": "./fesm2015/flex.mjs",
      "node": "./fesm2015/flex.mjs",
      "default": "./fesm2020/flex.mjs"
    },
    "./grid": {
      "types": "./grid.d.ts",
