    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@angular/cdk": "^13.0.0",
    "@angular/core": "0.0.0-NG",
    "@angular/common": "0.0.0-NG",
    "@angular/platform-browser": "0.0.0-NG",
    "rxjs": "^6.5.3 || ^7.4.0"
  },
  "sideEffects": false
